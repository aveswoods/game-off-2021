[application]

config/name="Game Off 2021"
run/main_scene="res://Scenes/GameRoot.tscn"

[autoload]

Global="*res://Global/Global.tscn"
RoomInfo="*res://Global/RoomInfo.gd"
SaveData="*res://Global/SaveData.gd"
Items="*res://Global/Items.gd"

[display]

